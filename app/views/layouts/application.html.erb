<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title><%= meta_title %></title>
    <meta name="description" content="<%= meta_description %>">

    <!-- Facebook Open Graph data -->
    <meta property="og:title" content="<%= meta_title %>" />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="<%= request.original_url %>" />
    <meta property="og:image" content="<%= meta_image %>" />
    <meta property="og:description" content="<%= meta_description %>" />
    <meta property="og:site_name" content="<%= meta_title %>" />

    <!-- Twitter Card data -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:site" content="<%= DEFAULT_META["twitter_account"] %>">
    <meta name="twitter:title" content="<%= meta_title %>">
    <meta name="twitter:description" content="<%= meta_description %>">
    <meta name="twitter:creator" content="<%= DEFAULT_META["twitter_account"] %>">
    <meta name="twitter:image:src" content="<%= meta_image %>">


    <script type="application/ld+json">
      {
        "@context": "https://schema.org",
        "@type": "NightClub",
        "image": [
          "#{request.protocol}#{request.host_with_port}#{asset_path("profile-wechat.jpg")}"
         ],
        "@id": "https://www.pinkygirls.co.nz/",
        "name": "Pinky Girls",
        "description": "Best Brothel in Auckland many gorgeous escorts available .",
        "address": {
          "@type": "PostalAddress",
          "streetAddress": "336 New North Rd",
          "addressLocality": "Auckland",
          "addressRegion": "Kingsland",
          "postalCode": "1021",
          "addressCountry": "NZ"
        },
        "geo": {
          "@type": "GeoCoordinates",
          "latitude": -36.8690526,
          "longitude": 174.7507902
        },
        "url": "https://www.pinkygirls.co.nz/",
        "telephone": "+0211255871",
        "openingHoursSpecification": [
          {
            "@type": "OpeningHoursSpecification",
            "dayOfWeek": [
              "Monday",
              "Sunday"
            ],
            "opens": "12:00",
            "closes": "06:00"
          }]
        },
      }
    </script>
    <%= csrf_meta_tags %>
    <%= action_cable_meta_tag %>
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <link href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">
    <%= stylesheet_pack_tag 'map' %>


    <%#= stylesheet_pack_tag 'application', media: 'all' %> <!-- Uncomment if you import CSS in app/javascript/packs/application.js -->
  </head>
  <body>
     <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@type": "BreadcrumbList",
      "itemListElement": [{
        "@type": "ListItem",
        "position": 1,
        "name": "Brothel auckland",
        "item": "https://www.pinkygirls.co.nz/"
      },{
        "@type": "ListItem",
        "position": 1,
        "name": "job",
        "item": "https://www.pinkygirls.co.nz/brothel/auckland/job"
      },{
        "@type": "ListItem",
        "position": 1,
        "name": "wechat contact",
        "item": "https://www.pinkygirls.co.nz/brothel/auckland/wechat"
      },{
        "@type": "ListItem",
        "position": 2,
        "name": "Escorts auckland",
        "item": "https://www.pinkygirls.co.nz/brothel/auckland/escorts"
      },{
        "@type": "ListItem",
        "position": 3,
        "name": "Escorts auckland",
        "item": "https://www.pinkygirls.co.nz/brothel/auckland/escorts/asian"
      }]
    }
    </script>
    <%= render 'shared/navbar' %>
    <%= render 'shared/flashes' %>
    <%= yield %>
    <%= render 'shared/footer' %>
    <%= javascript_include_tag 'application' %>
    <%= javascript_pack_tag 'application' %>
    <%= javascript_include_tag 'app/jquery.js' %>
    <%= javascript_include_tag 'app/jquery.magnific-popup.min.js' %>
    <%= javascript_include_tag 'app/jquery.stellar.min.js' %>
    <%= javascript_include_tag 'app/bootstrap.min.js' %>
    <%= javascript_include_tag 'app/owl.carousel.min.js' %>

    <%= javascript_include_tag 'app/smoothscroll.js' %>
    <%= javascript_include_tag 'app/wow.min.js' %>
    <%= javascript_include_tag 'app/custom.js' %>
    <%= javascript_pack_tag 'map' %>

    <%= render 'shared/analytics' %>


  </body>
</html>
