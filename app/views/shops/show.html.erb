  <section id="home" class="slider" data-stellar-background-ratio="0.5">
    <div class="row ">
      <div class="home-video">
        <div class="col-xs-12">
          <video loop autoplay muted class="web-video">
            <!-- <source id="v1" src="https://res.cloudinary.com/pinky-girls/video/upload/br_5811k,c_crop,dl_21,h_614,so_15,w_1280/v1549765593/Club_Pinky-Girls-3w-7yZ8pAD4.mp4" type="video/mp4"> -->
            <!-- <source src="https://res.cloudinary.com/pinky-girls/video/upload/c_scale,g_center,h_430,q_auto:best,so_15,w_475,y_114/v1549765593/Club_Pinky-Girls-3w-7yZ8pAD4.mp4" type="video/mp4">
            -->
            Your browser does not support HTML5 video.
          </video>
          <video loop autoplay muted playsinline class="mobile-video" >
           <!--  <source  src="https://res.cloudinary.com/pinky-girls/video/upload/c_scale,g_center,h_430,q_auto:best,so_15,w_475,y_114/v1549765593/Club_Pinky-Girls-3w-7yZ8pAD4.mp4" type="video/mp4"> -->
            <!-- <source src="https://res.cloudinary.com/pinky-girls/video/upload/c_scale,g_center,h_430,q_auto:best,so_15,w_475,y_114/v1549765593/Club_Pinky-Girls-3w-7yZ8pAD4.mp4" type="video/mp4">
            -->
            Your browser does not support HTML5 video.
          </video>
        </div>
        <div class="col-xs-12 col-md-4 col-sm-8 hours">
          <div class="footer-info footer-open-hour">
            <div class="section-title">
              <h2 class="wow fadeInUp" data-wow-delay="0.2s"><%= t(".hours")%></h2>
            </div>
            <div class="wow fadeInUp" data-wow-delay="0.4s">
              <!-- <p>Monday: Closed</p> -->
              <br>
              <br>
              <!-- <div>
                <strong>Tuesday to Friday</strong>
                <p>7:00 AM - 9:00 PM</p>
              </div> -->
              <div>
                <strong><%= t(".days")%></strong>
                <p><%= t(".times")%></p>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="wave img-responsive"></div>
    </div>
  </section>
</div>
<!-- ABOUT -->
<% cache @shop do %>
  <div class="container">
    <section id="about" data-stellar-background-ratio="0.5">
      <div class="container">
        <div class="row">
          <div class="col-md-6 col-sm-12">
            <div class="about-info">
              <div class="section-title wow fadeInUp" data-wow-delay="0.2s">
                <h4><%= t(".paradise") %></h4>
                <a href="tel:+64<%= @shop.phone %>"><%= t(".call") %> <i class="fa fa-phone"></i> 0<%= @shop.phone %></a>
                <h2><%= @shop.address %></h2>
              </div>
              <div class="wow fadeInUp" data-wow-delay="0.4s">
                <p><%= @shop.description %></p>
              </div>
            </div>
          </div>
          <div class="col-xs-12 col-md-6 col-sm-12 ">
            <div class="wow fadeInUp " data-wow-delay="0.6s">
              <%= image_tag "final-heart-map.png", class: "heart-map" %>
              <div   id="map"
                style="width: 100%;
                height: 480px;
                border-radius: 20px;"
                data-markers="<%= @markers.to_json %>"
                data-mapbox-api-key="<%= ENV['MAPBOX'] %>"
              ></div>

            </div>
          </div>
        </div>
      </div>
    </section>
  </div>
  <!-- MENU-->
  <div class="container">
    <div class="row">
      <div class="card background card-shop" style="background-image: url(<%= asset_url 'girls.png' %>);">
        <%= link_to "", shop_girls_path(@shop), class: "card-link" %>
      </div>
      <%= link_to "Girls", shop_girls_path(@shop), class: "btn-girls btn btn-primary" %>
        <div class="col-xs-12 col-md-4 col-sm-8 girls">
          <%= link_to shop_girls_path(@shop) do %>
          <div class="footer-info footer-open-hour">
            <div class="section-title">
              <h2 class="wow fadeInUp" data-wow-delay="0.2s"><%= t(".girls") %></h2>
            </div>
            <div class="wow fadeInUp" data-wow-delay="0.4s">
              <div class="btn btn-primary">
                <%= t(".girls-text") %>
              </div>
            </div>
          </div>
          <% end %>
        </div>

    </div>
  </div>
  <!-- TEAM -->
  <div class="container">
    <section id="team"   class="team-position">
      <div class="container">
        <div class="row">
          <div class="col-md-12 col-sm-12">
            <div class="section-title wow fadeInUp" data-wow-delay="0.1s">
              <h2><%= t(".meet")%></h2>
              <h4><%= t(".friendly")%></h4>
            </div>
          </div>
          <div class="col-md-12 col-sm-12">
            <div class="team-thumb wow fadeInUp wechat-tumb" data-wow-delay="0.2s">
              <%= link_to shop_wechat_path(@shop) do %>
              <%= image_tag "wechat-contact.png", class: "img-responsive" %>
              <div class="team-hover">
                <div class="team-item">
                  <h4><%= t(".team-item-wechat") %></h4>
                  <ul class="social-icon">
                    <li><%= image_tag "pinkygirls.png"  %></li>
                  </ul>
                </div>
              </div>
              <% end %>
            </div>
            <div class="team-info">
              <h3><%= t(".contact-wechat") %></h3>
              <p><%= t(".moments") %></p>
            </div>
          </div>
        </div>
      </div>
    </section>
  </div>
<% end %>
