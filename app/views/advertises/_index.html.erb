<div class="container">
  <div class="row">
    <div class="col-xs-12 col-md-offset-2 col-md-10">
      <h1> </h1>
      <div class="aa_htmlTable">
        <h2 class="aa_h2"><%= t(".table-name") %></h2>
        <table>
          <thead>
            <tr>
              <th><%= t(".table-hour") %></th>
              <th><%= t(".money")%></th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td><%= t(".time-1") %></td>
              <td>$<%= @pricing.half %></td>
            </tr>
            <tr>
              <td><%= t(".time-4") %></td>
              <td>$<%=  @pricing.forty_five  %></td>
            </tr>
            <tr>
              <td><%= t(".time-2") %></td>
              <td>$<%= @pricing.hour %></td>
            </tr>
          </tbody>
          <tfoot>
            <tr>
              <td><%= t(".time-3") %></td>
              <td>$<%= @pricing.outcall %></td>
            </tr>
          </tfoot>
        </table>
      </div>
    </div>
  </div>
</div>
